<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.talentica.com/hungryhippos/config/coordination"
	xmlns:tns="http://www.talentica.com/hungryhippos/config/coordination"
	elementFormDefault="qualified">
	<element name="coordination-config">
		<complexType>
			<sequence>
				<element type="tns:zookeeper-default-config" name="zookeeper-default-config"/>
			</sequence>
		</complexType>
	</element>
	
	<complexType name="zookeeper-default-config">
		<sequence>
			<element name="namespace-path" type="string" default="/rootnode" />
			<element name="filesystem-path" type="string" default="/rootnode/file-system" />
			<element name="cleanup" type="string" default="Y" />
		</sequence>
	</complexType>
	
</schema>