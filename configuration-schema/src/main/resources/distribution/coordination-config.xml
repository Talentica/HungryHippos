<?xml version="1.0" encoding="UTF-8"?>
<tns:coordination-config
	xmlns:tns="http://www.talentica.com/hungryhippos/config/coordination"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.talentica.com/hungryhippos/config/coordination coordination-config.xsd ">
	<tns:zookeeper-default-config>
		<tns:tick-time>5000</tns:tick-time>
		<tns:retry>5</tns:retry>
		<tns:namespace-path>hungryhippos</tns:namespace-path>
		<tns:host-path>/hungryhippos/hostsnode</tns:host-path>
		<tns:alert-path>/hungryhippos/alertsnode</tns:alert-path>
		<tns:filesystem-path>/hungryhippos/file-system</tns:filesystem-path>
		<tns:sharding-table-path>/hungryhippos/sharding-table</tns:sharding-table-path>
        <tns:job-config-path>/hungryhippos/job-config</tns:job-config-path>
        <tns:job-status-path>/hungryhippos/job-status</tns:job-status-path>
		<tns:cleanup>Y</tns:cleanup>
	</tns:zookeeper-default-config>
</tns:coordination-config>