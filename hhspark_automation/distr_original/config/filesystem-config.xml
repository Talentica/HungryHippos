<?xml version="1.0" encoding="UTF-8"?>
<tns:file-system-config xmlns:tns="http://www.talentica.com/hungryhippos/config/filesystem" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.talentica.com/hungryhippos/config/filesystem filesystem-config.xsd ">
  <tns:root-directory>/home/hhuser/hh/filesystem</tns:root-directory>
	<tns:data-file-prefix>data_</tns:data-file-prefix>
	<tns:append-to-data-file>false</tns:append-to-data-file>
	<tns:server-port>9898</tns:server-port>
	<tns:query-retry-interval>2500</tns:query-retry-interval>
	<tns:max-query-attempts>10</tns:max-query-attempts>
	<tns:file-stream-buffer-size>2048</tns:file-stream-buffer-size>
	<tns:max-client-requests>25</tns:max-client-requests>
</tns:file-system-config>
